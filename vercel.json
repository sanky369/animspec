{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/analyze/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/upload/route.ts": {
      "maxDuration": 30,
      "memory": 512
    },
    "src/app/api/checkout/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/webhooks/lemon-squeezy/route.ts": {
      "maxDuration": 30
    }
  },
  "crons": [],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
